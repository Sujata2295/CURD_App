@model IEnumerable<State_New.Models.CityModel>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.City_Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.City_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.State_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsActive)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.City_Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.City_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.State_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsActive)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new {  id=item.City_Id  }) |
                @Html.ActionLink("Details", "Details", new {  id=item.City_Id  }) |
                @Html.ActionLink("Delete", "Delete", new {  id=item.City_Id  })
            </td>
        </tr>
}
    </tbody>
</table>
